    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Menu x:Name="mMainMenu" Grid.Row="0">
            <MenuItem Header="_SIGENCE Scenario Tool">
                <MenuItem Header="_New File" Icon="{StaticResource NEW}" Command="{x:Static ApplicationCommands.New}" />
                <Separator />
                <MenuItem Header="_Open File" Icon="{StaticResource OPEN}" Command="{x:Static ApplicationCommands.Open}" />
                <MenuItem Header="Recent Files" x:Name="miRecentFiles" Icon="{StaticResource OPEN}" />
                <Separator />
                <MenuItem Header="_Save File" Icon="{StaticResource SAVE}" Command="{x:Static ApplicationCommands.Save}" />
                <MenuItem Header="_Save File As" Icon="{StaticResource SAVEAS}" Command="{x:Static ApplicationCommands.SaveAs}" />
                <Separator />
                <MenuItem Header="Quit" Icon="{StaticResource EXIT}" Command="{x:Static ApplicationCommands.Close}" />
            </MenuItem>
            <MenuItem Header="_Actions">
                <MenuItem Header="_Create RFDevice" Icon="{StaticResource ADD}" Command="{x:Static cmd:RegisteredCommands.CreateRFDevice}" />
                <MenuItem Header="_Delete RFDevice" Icon="{StaticResource DELETE}" Command="{x:Static cmd:RegisteredCommands.DeleteRFDevice}" />
                <Separator />
                <MenuItem Header="Copy RFDevice" Icon="{StaticResource COPY}" Command="{x:Static cmd:RegisteredCommands.CopyRFDevice}" />
                <MenuItem Header="Paste RFDevice" Icon="{StaticResource PASTE}" Command="{x:Static cmd:RegisteredCommands.PasteRFDevice}" />
                <Separator />
                <MenuItem Header="_Move RFDevice" Icon="{StaticResource MOVE}" IsCheckable="True" IsChecked="{Binding IsDeviceMovingMode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <Separator />
                <MenuItem Header="_Export RFDevices" Icon="{StaticResource EXPORT}" Command="{x:Static cmd:RegisteredCommands.ExportRFDevice}" />
                <MenuItem Header="_Import RFDevices" Icon="{StaticResource IMPORT}" Command="{x:Static cmd:RegisteredCommands.ImportRFDevice}" />
            </MenuItem>
            <MenuItem Header="_Tools">
                <MenuItem Header="_Sync Map And Grid Selection" Icon="{StaticResource NUT_AND_BOLT}" IsCheckable="True" IsChecked="{Binding SyncMapAndGrid,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                <MenuItem Header="_Zoom To RFDevice" Icon="{StaticResource ZOOMTO}" Command="{x:Static cmd:RegisteredCommands.ZoomToRFDevice}" />
                <MenuItem Header="_Create Screenshot From Map" Icon="{StaticResource CAMERA}" Command="{x:Static cmd:RegisteredCommands.CreateScreenshot}" />
                <MenuItem Header="Create _RFDevices Along A Line" Icon="{StaticResource MAGIC_WAND}" IsCheckable="True" IsChecked="{Binding StartedDALF,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
                <!--<Separator />
                <MenuItem Header="_Send Data via UDP" Icon="{StaticResource SEND}" Command="{x:Static cmd:RegisteredCommands.SendDataUDP}" />-->
                <Separator />
                <MenuItem Header="_Open In Google Maps" Icon="{StaticResource PIN_BLUE}" Command="{x:Static cmd:RegisteredCommands.OpenInGoogleMaps}" />
                <MenuItem Header="_QRCode From RFDevice" Icon="{StaticResource CAMERA}" Command="{x:Static cmd:RegisteredCommands.RFDeviceQRCode}" />
                <MenuItem Header="Open _Scripting Editor" Icon="{StaticResource TEXT_RICH_COLORED}" Command="{x:Static cmd:RegisteredCommands.OpenScriptEditor}" />
            </MenuItem>
            <MenuItem Header="_Extras">
                <MenuItem Header="Toggle Info _Window" Icon="{StaticResource TEXT_RICH_COLORED}" Command="{x:Static cmd:RegisteredCommands.ToggleInfoWindow}" />
                <Separator />
                <MenuItem Header="_Restore Initial Map Values" Icon="{StaticResource NUT_AND_BOLT}" Click="MenuItem_RestoreInitialMapValues_Click" />
                <MenuItem Header="_Save Initial Map Values" Icon="{StaticResource NUT_AND_BOLT}" Click="MenuItem_SaveInitialMapValues_Click" />
                <Separator />
                <MenuItem Header="_Edit Settings" Icon="{StaticResource NUT_AND_BOLT}" Command="{x:Static cmd:RegisteredCommands.OpenSettings}" />
            </MenuItem>
            <MenuItem x:Name="miTest" Header="_Test">
                <MenuItem Header="Scenario Simulation _Player" Icon="{StaticResource TEXT_RICH_COLORED}" Click="MenuItem_ScenarioSimulationPlayer_Click" />
                <!--<MenuItem Header="Toggle Data _Grid" Icon="{StaticResource TEXT_RICH_COLORED}" Command="{x:Static cmd:RegisteredCommands.ToggleDataGrid}" />-->
                <Separator />
                <MenuItem Header="_Load Streets" Icon="{StaticResource EARTH}" Click="MenuItem_LoadStreets_Click" />
                <MenuItem Header="_Heatmap Test" Icon="{StaticResource EARTH}" Click="MenuItem_HeatmapTest_Click" />
                <MenuItem Header="_Charting Test" Icon="{StaticResource CHART}" Click="MenuItem_ChartingTest_Click" />
                <MenuItem Header="Create Scenario _Report" Icon="{StaticResource TEXT_RICH_COLORED}" Click="MenuItem_ScenarioReport_Click" />
                <Separator />
                <MenuItem Header="Create Randomized RFDevices" Icon="{StaticResource SATELLITE}">
                    <MenuItem Header="50" Tag="50" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                    <MenuItem Header="100" Tag="100" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                    <MenuItem Header="200" Tag="200" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                    <MenuItem Header="500" Tag="500" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                    <MenuItem Header="1000" Tag="1000" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                    <MenuItem Header="2000" Tag="1000" Icon="{StaticResource SATELLITE}" Click="MenuItem_CreateSomeRandomizedRFDevices_Click" />
                </MenuItem>
            </MenuItem>
            <MenuItem Header="_Help">
                <!--<MenuItem Header="_Display Help" Command="{x:Static ApplicationCommands.Help}" IsEnabled="False" />-->
                <MenuItem Header="_Cheat Sheet" Icon="{StaticResource HELP}" Command="{x:Static cmd:RegisteredCommands.OpenCheatSheet}" />
                <MenuItem Header="_About" Icon="{StaticResource HELP}" Click="MenuItem_About_Click" />
                <Separator />
                <MenuItem Header="_Open Issues" Icon="{StaticResource BUG_GREEN}" Click="MenuItem_OpenIssues_Click" />
                <MenuItem Header="_Open Kanban Board" Icon="{StaticResource TABLE_INFORMATION}" Click="MenuItem_KanbanBoard_Click" />
                <!--<MenuItem Header="_About" IsEnabled="False" />-->
                <!--<Separator />
                <MenuItem Header="Check _Version" Icon="{StaticResource HELP}" Click="MenuItem_CheckVersion_Click" />-->
            </MenuItem>
        </Menu>

        <ToolBar Grid.Row="1">

            <Button Content="{StaticResource NEW}" ToolTip="New File" Command="{x:Static ApplicationCommands.New}" />
            <Button Content="{StaticResource OPEN}" ToolTip="Open File" Command="{x:Static ApplicationCommands.Open}" />
            <Button Content="{StaticResource SAVE}" ToolTip="Save File" Command="{x:Static ApplicationCommands.Save}" />

            <Separator />

            <Label Content="MapProvider" />
            <ComboBox x:Name="cbMapProvider" Width="120" SelectedItem="{Binding MapProvider, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTip="Here You Can Change The MapProvider For The Map." Margin="3" />
            <ToggleButton Content="+" ToolTip="Display The Center Marker In The Map" Foreground="Red" FontWeight="ExtraBold" IsChecked="{Binding ShowCenter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

            <Separator />

            <Button Content="{StaticResource ADD}" ToolTip="Create RFDevice" Command="{x:Static cmd:RegisteredCommands.CreateRFDevice}" />
            <Button Content="{StaticResource DELETE}" ToolTip="Delete RFDevice" Command="{x:Static cmd:RegisteredCommands.DeleteRFDevice}" />
            <ToggleButton Content="{StaticResource MOVE}" ToolTip="Move RFDevice" IsChecked="{Binding IsDeviceMovingMode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

            <Separator />

            <Button Content="{StaticResource COPY}" ToolTip="Copy RFDevice" Command="{x:Static cmd:RegisteredCommands.CopyRFDevice}" />
            <Button Content="{StaticResource PASTE}" ToolTip="Paste RFDevice" Command="{x:Static cmd:RegisteredCommands.PasteRFDevice}" />

            <Separator />

            <Button Content="{StaticResource ZOOMTO}" ToolTip="Zoom To RFDevice" Command="{x:Static cmd:RegisteredCommands.ZoomToRFDevice}" />
            <ToggleButton Content="{StaticResource MAGIC_WAND}" ToolTip="Create RFDevices Along A Line" IsChecked="{Binding StartedDALF,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
            <ToggleButton Content="{StaticResource NUT_AND_BOLT}" ToolTip="Sync Map And Grid Selection" IsChecked="{Binding SyncMapAndGrid,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
            <Button Content="{StaticResource CAMERA}" ToolTip="Create Screenshot" Command="{x:Static cmd:RegisteredCommands.CreateScreenshot}" />

            <!--<Separator />

            <ToggleButton Content="{StaticResource SERVER_FROM_CLIENT}" ToolTip="Receive Data Via UDP" IsChecked="{Binding IsReceiveDataUDP,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />
            <Button Content="{StaticResource SERVER_TO_CLIENT}" ToolTip="Send Data Via UDP" Command="{x:Static cmd:RegisteredCommands.SendDataUDP}" />-->

            <Separator />

            <Button Content="{StaticResource OPEN}" ToolTip="Open Templates" Click="MenuItem_LoadTemplates_Click" />
            <Button Content="{StaticResource SAVE}" ToolTip="Save Templates" Click="MenuItem_SaveTemplates_Click" />
            <Label Content="RFDevice Template" />
            <ComboBox x:Name="cbTemplate" Width="120" ItemsSource="{Binding RFDeviceTemplateCollection}" SelectedItem="{Binding CurrentSelectedTemplate}" ToolTip="Here You Can Select The Template For New RFDevices." Margin="3" />
            <Button Content="{StaticResource STAR_ADD}" ToolTip="Add To Templates" Click="MenuItem_AddToTemplates_Click" />
            <Button Content="{StaticResource STAR_DEL}" ToolTip="Del From Templates" Click="MenuItem_DelFromTemplates_Click" />

            <Separator />

            <CheckBox Content="Simulation" IsChecked="{Binding SimulationMode,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" ToolTip="A Switch To Change The Mode From Simulation To Physical." />

            <Separator />
        </ToolBar>


        <TabControl x:Name="tcTabControl" Grid.Row="2" SelectionChanged="TabControl_SelectionChanged">

            <TabItem x:Name="tiMap">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal">
                        <Label Content="{StaticResource EARTH}" />
                        <Label Content="Map" />
                    </StackPanel>
                </TabItem.Header>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="2*" />
                        <RowDefinition Height="1*" />
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <gmap:GMapControl x:Name="mcMapControl" Grid.Row="0" OnPositionChanged="MapControl_OnPositionChanged" MouseLeftButtonDown="MapControl_MouseLeftButtonDown" MouseRightButtonDown="MapControl_MouseRightButtonDown" OnMapZoomChanged="MapControl_OnMapZoomChanged" />

                        <!--<Border BorderBrush="Black" BorderThickness="2" Width="240" Height="200" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="20" Background="AliceBlue" Visibility="{Binding InfoWindowVisibility}">-->
                        <Border BorderBrush="Black" BorderThickness="2" CornerRadius="10" Opacity="0.65" Width="240" Height="200" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="20" Background="Black" Visibility="{Binding InfoWindowVisibility}">

                            <Border.Effect>
                                <DropShadowEffect />
                            </Border.Effect>

                            <Grid>

                                <Grid.Resources>
                                    <Style x:Key="Header">
                                        <Style.Setters>
                                            <Setter Property="Label.FontWeight" Value="Bold" />
                                            <Setter Property="Label.Foreground" Value="White" />
                                            <!--<Setter Property="Label.HorizontalContentAlignment" Value="Right" />-->
                                        </Style.Setters>
                                    </Style>
                                    <Style x:Key="Data">
                                        <Style.Setters>
                                            <Setter Property="Label.HorizontalContentAlignment" Value="Right" />
                                            <Setter Property="Label.Foreground" Value="White" />
                                        </Style.Setters>
                                    </Style>
                                </Grid.Resources>

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>

                                <Label Grid.Column="0" Grid.Row="0" Content="Id" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="0" Content="{Binding CurrentSelectedDevice.Id}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="1" Content="Name" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="1" Content="{Binding CurrentSelectedDevice.Name}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="2" Content="RxTxType" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="2" Content="{Binding CurrentSelectedDevice.RxTxType}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="3" Content="AntennaType" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="3" Content="{Binding CurrentSelectedDevice.AntennaType}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="4" Content="CenterFrequency" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="4" Content="{Binding CurrentSelectedDevice.HumanCenterFrequency}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="5" Content="Bandwidth" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="5" Content="{Binding CurrentSelectedDevice.HumanBandwidth}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="6" Content="Gain" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="6" Content="{Binding CurrentSelectedDevice.HumanGain}" Style="{StaticResource Data}" />

                                <Label Grid.Column="0" Grid.Row="7" Content="SignalToNoiseRatio" Style="{StaticResource Header}" />
                                <Label Grid.Column="1" Grid.Row="7" Content="{Binding CurrentSelectedDevice.HumanSignalToNoiseRatio}" Style="{StaticResource Data}" />

                            </Grid>
                        </Border>
                    </Grid>

                    <Grid Grid.Row="1">

                        <Grid.RowDefinitions>

                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <ToolBar Grid.Row="0" Margin="5">

                            <Label Content="Filter" FontWeight="Bold" />

                            <Separator />

                            <CheckBox Content="Receiver" Width="80" IsChecked="{Binding ShowReceiver,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" BorderThickness="1" BorderBrush="Black" Margin="5,0,0,0" />
                            <CheckBox Content="Transmitter" Width="80" IsChecked="{Binding ShowTransmitter,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" BorderThickness="1" BorderBrush="Black" Margin="5,0,0,0" />

                            <Label Content="Id" Margin="5,0,0,0" />
                            <TextBox Text="{Binding IdFilter,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="80" TextAlignment="Right" VerticalContentAlignment="Center" BorderThickness="1" BorderBrush="Black" />

                            <Label Content="RxTxType" Margin="5,0,0,0" />
                            <ComboBox x:Name="cbRxTxType" SelectedItem="{Binding RxTxTypeFilter, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="120" />

                            <Label Content="AntennaType" Margin="5,0,0,0" ToolTip="This Filter Is Currently Not Yet Implemented!" />
                            <ComboBox x:Name="cbAntennaType" Width="120" IsEnabled="False" ToolTip="This Filter Is Currently Not Yet Implemented!" />

                            <Separator />
                            <Button Content="Reset All Filter" BorderThickness="1" BorderBrush="Black" Margin="5,0,0,0" Click="Button_ResetAllDeviceFilter_Click" />
                        </ToolBar>


                        <DataGrid x:Name="dgRFDevices" Grid.Row="1" ItemsSource="{Binding RFDeviceViewModelCollection}" IsReadOnly="False" AlternationCount="2" AlternatingRowBackground="AliceBlue" AutoGenerateColumns="False" FontFamily="Courier New" FontSize="14" CanUserAddRows="False" CanUserDeleteRows="False" SelectionMode="Extended" KeyDown="DataGrid_KeyDown" BeginningEdit="DataGrid_BeginningEdit" CellEditEnding="DataGrid_CellEditEnding" SelectionChanged="DataGrid_SelectionChanged" PreviewKeyDown="DataGrid_PreviewKeyDown">

                            <DataGrid.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="_Move RFDevice" Icon="{StaticResource MOVE}" IsCheckable="True" IsChecked="{Binding IsDeviceMovingMode, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                                    <MenuItem Header="_Delete RFDevice" Icon="{StaticResource DELETE}" Command="{x:Static cmd:RegisteredCommands.DeleteRFDevice}" />
                                    <Separator />
                                    <MenuItem Header="_Copy RFDevice" Icon="{StaticResource COPY}" Command="{x:Static cmd:RegisteredCommands.CopyRFDevice}" />
                                    <MenuItem Header="_Paste RFDevice" Icon="{StaticResource PASTE}" Command="{x:Static cmd:RegisteredCommands.PasteRFDevice}" />
                                    <Separator />
                                    <MenuItem Header="_Zoom To RFDevice" Icon="{StaticResource ZOOMTO}" Command="{x:Static cmd:RegisteredCommands.ZoomToRFDevice}" />
                                    <MenuItem Header="_QRCode From RFDevice" Icon="{StaticResource CAMERA}" Command="{x:Static cmd:RegisteredCommands.RFDeviceQRCode}" />
                                    <MenuItem Header="_Open In Google Maps" Icon="{StaticResource PIN_BLUE}" Command="{x:Static cmd:RegisteredCommands.OpenInGoogleMaps}" />
                                    <Separator />
                                    <MenuItem Header="_Add To Favorites" Icon="{StaticResource STAR_ADD}" Click="MenuItem_AddToTemplates_Click" />
                                </ContextMenu>
                            </DataGrid.ContextMenu>

                            <DataGrid.CommandBindings>

                                <CommandBinding Command="{x:Static ApplicationCommands.Copy}" CanExecute="DataGrid_CommandBinding_CanExecute_Copy" Executed="DataGrid_Execute_Copy" />
                                <CommandBinding Command="{x:Static ApplicationCommands.Paste}" CanExecute="DataGrid_CommandBinding_CanExecute_Paste" Executed="DataGrid_Execute_Paste" />
                            </DataGrid.CommandBindings>

                            <DataGrid.Columns>

                                <DataGridCheckBoxColumn Header="" Binding="{Binding IsMarked,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
                                    <DataGridCheckBoxColumn.ElementStyle>
                                        <Style TargetType="CheckBox">
                                            <Setter Property="HorizontalAlignment" Value="Center" />
                                            <Setter Property="Margin" Value="5" />
                                        </Style>
                                    </DataGridCheckBoxColumn.ElementStyle>
                                </DataGridCheckBoxColumn>

                                <DataGridTextColumn Header="" Width="10" IsReadOnly="True">
                                    <DataGridTextColumn.ElementStyle>
                                        <Style TargetType="TextBlock">
                                            <Setter Property="HorizontalAlignment" Value="Center" />
                                            <Setter Property="Margin" Value="5" />
                                            <Setter Property="Text" Value=" " />
                                            <Setter Property="Background" Value="{Binding ValidationBackground}" />
                                            <Setter Property="ToolTip" Value="{Binding ValidationHint}" />
                                        </Style>
                                    </DataGridTextColumn.ElementStyle>
                                </DataGridTextColumn>

                                <DataGridTextColumn Header="Type" Binding="{Binding DeviceType,Mode=OneWay}">
                                    <!--<DataGridTextColumn.HeaderStyle>
                                        <Style TargetType="{x:Type DataGridColumnHeader}">
                                            <Setter Property="ToolTip" Value="{Binding Source={StaticResource ToolTips},Path=TOOLTIP_}" />
                                        </Style>
                                    </DataGridTextColumn.HeaderStyle>-->
                                    <DataGridTextColumn.ElementStyle>
                                        <Style TargetType="TextBlock">
                                            <!--<Setter Property="HorizontalAlignment" Value="Center" />-->
                                            <Setter Property="Margin" Value="5" />
                                            <!--<Setter Property="FontWeight" Value="Bold" />-->
                                        </Style>
                                    </DataGridTextColumn.ElementStyle>
                                </DataGridTextColumn>

                                <DataGridTextColumn Header="Id" Binding="{Binding Id,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}">
