        </TabControl>


        <StatusBar Grid.Row="3">

            <StatusBarItem>
                <Slider Width="120" Minimum="2" Maximum="22" Value="{Binding Zoom}" TickFrequency="1" IsSnapToTickEnabled="True" TickPlacement="Both" />
            </StatusBarItem>

            <Separator />

            <StatusBarItem>
                <StackPanel Orientation="Horizontal">
                    <Label Content="Zoom:" />
                    <Label Content="{Binding Zoom}" />
                </StackPanel>
            </StatusBarItem>

            <Separator />

            <StatusBarItem>
                <StackPanel Orientation="Horizontal">
                    <Label Content="CenterPosition:" />
                    <Label Content="{Binding Latitude}" ContentStringFormat="F8" />
                    <Label Content="/" />
                    <Label Content="{Binding Longitude}" ContentStringFormat="F8" />
                </StackPanel>
            </StatusBarItem>

            <Separator />

            <StatusBarItem>
                <ProgressBar Width="80" Height="10" IsIndeterminate="{Binding IsTileLoading}" ToolTip="Indicator Whether Tiles Are Loaded" />
            </StatusBarItem>

            <Separator />

            <StatusBarItem>
                <StackPanel Orientation="Horizontal">
                    <Label Content="RFDevices:" />
